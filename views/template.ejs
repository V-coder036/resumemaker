<!DOCTYPE html>
<html>
  <head>
    <title><%=name%> - Curriculum Vitae</title>
    

    <meta name="viewport" content="width=device-width" />
    <meta charset="UTF-8" />

    <link rel="stylesheet" href="/css/style.css">
    <link
      href="http://fonts.googleapis.com/css?family=Rokkitt:400,700|Lato:400,300"
      rel="stylesheet"
      type="text/css"
    >

    
  </head>
  <body id="top">
    <div id="cv" class="instaFade">
      <div class="mainDetails">
        <div id="name">
          <h1 class="quickFade delayTwo"><%=name%></h1>
        </div>

        <div id="contactDetails" class="quickFade delayFour">
          <ul>
            <li>
              e:
              <a href=<"<%=email%>"> target="_blank"><%=email%></a>
            </li>
            <li>w: <a href=<%=portlink%>><%=portlink%></a></li>
            <li>m: <%=phoneno%></li>
            <li>a: <%=address%></li>
          </ul>
        </div>
        <div class="clear"></div>
      </div>

      <div id="mainArea" class="quickFade delayFive">
        <section>
          <article>
            <div class="sectionTitle">
              <h1>Personal Profile</h1>
            </div>

            <div class="sectionContent">
              <p>
                <%=persprof%>
              </p>
            </div>
          </article>
          <div class="clear"></div>
        </section>

        <section>
          <div class="sectionTitle">
            <h1>Work Experience</h1>
          </div>

          <div class="sectionContent">
            <article>
              <h2><%=wname1%></h2>
              <p class="subDetails"><%=wtime1%></p>
              <p>
                <%=wdesc1%>
              </p>
            </article>

            <article>
              <h2><%=wname2%></h2>
              <p class="subDetails"><%=wtime2%></p>
              <p>
                <%=wdesc2%>
              </p>
            </article>

            <article>
              <h2><%=wname3%></h2>
              <p class="subDetails"><%=wtime3%></p>
              <p>
                <%=wdesc3%>
              </p>
            </article>
          </div>
          <div class="clear"></div>
        </section>

        <section>
          <div class="sectionTitle">
            <h1>Projects</h1>
          </div>

          <div class="sectionContent">
            <article>
              <h2><%=pname1%></h2>
              <p class="subDetails"><%=ptime1%></p>
              <p>
                <%=pdesc1%>
              </p>
            </article>

            <article>
              <h2><%=pname2%></h2>
              <p class="subDetails"><%=ptime2%></p>
              <p>
                <%=pdesc2%>
              </p>
            </article>

            <article>
              <h2><%=pname3%></h2>
              <p class="subDetails"><%=ptime3%></p>
              <p>
                <%=pdesc3%>
              </p>
            </article>
          </div>
          <div class="clear"></div>
        </section>

        <section>
          <div class="sectionTitle">
            <h1>Key Skills</h1>
          </div>

          <div class="sectionContent">
            <ul class="keySkills">
              <li><%=skill1%></li>
              <li><%=skill2%></li>
              <li><%=skill3%></li>
              <li><%=skill4%></li>
              <li><%=skill5%></li>
              <li><%=skill6%></li>
            </ul>
          </div>
          <div class="clear"></div>
        </section>

        <section>
          <div class="sectionTitle">
            <h1>Education</h1>
          </div>

          <div class="sectionContent">
            <article>
              <h2><%=sname1%></h2>
              <p class="subDetails"><%=squal1%></p>
              <p>
                <%=sinfo1%>
              </p>
            </article>

            <article>
              <h2><%=sname2%></h2>
              <p class="subDetails"><%=squal2%></p>
              <p>
                <%=sinfo2%>
              </p>
            </article>
          </div>
          <div class="clear"></div>
        </section>
      </div>
    </div>
  </body>
</html>
